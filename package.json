{
  "name": "react-redux-test-workshop",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kiragona.github.io/react-redux-test-workshop",
  "dependencies": {
    "gh-pages": "^1.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-modal": "^2.2.2",
    "react-redux": "^5.0.5",
    "react-router": "3",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.1.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "tape -r babel-register -r babel-polyfill \"{test/integration,src}/**/*.spec.js\" --tap | tap-spec",
    "coverage": "nyc --check-coverage npm test"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.9.1",
    "jsdom": "^11.1.0",
    "mock-css-modules": "^1.0.0",
    "nyc": "^11.6.0",
    "react-test-renderer": "^16.3.1",
    "sinon": "^4.5.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "nyc": {
    "statements": 90,
    "branches": 85,
    "functions": 90,
    "lines": 90,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "**/*.spec.js",
      "test/**/*.js"
    ],
    "cache": true
  }
}
